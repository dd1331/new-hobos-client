    "pinia": "^2.0.21",
    "qs": "^6.11.0",
    "roboto-fontface": "*",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.1",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
